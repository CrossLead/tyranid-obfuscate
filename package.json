{
  "name": "tyranid-obfuscate",
  "version": "0.0.1",
  "description": "tyranid-obfuscate",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:CrossLead/tyranid-obfuscate.git"
  },
  "keywords": [
    ""
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib && rimraf coverage && rimraf .nyc_output && rimraf lib_test",
    "prettier": "prettier --write \"{src,test}/**/*.ts\"",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "prepublish": "npm run build",
    "prebuild": "npm run clean && npm run prettier && npm run lint && echo Using TypeScript && tsc --version",
    "build": "tsc --pretty",
    "test": "npm run clean && tsc -p tsconfig.test.json --pretty && nyc --exclude \"**/*-spec.js\" ava \"**/*-spec.js\" --verbose",
    "coverage": "nyc report --reporter=lcov --reporter=text --reporter=html",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run test -- --watch"
  },
  "dependencies": {
    "@types/mongodb": "^3.1.19",
    "mongodb": "^3.1.10",
    "tyranid": "^0.5.66"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.5.0",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/express": "^4.16.1",
    "@types/mongodb-memory-server": "^1.8.0",
    "@types/node": "^7.10.3",
    "ava": "^0.20.0",
    "chai": "^4.2.0",
    "coveralls": "^2.0.0",
    "husky": "^1.3.1",
    "mocha": "^5.2.0",
    "mongodb-memory-server": "^3.0.0",
    "nyc": "^10.0.0",
    "prettier": "^1.5.2",
    "rimraf": "^2.0.0",
    "tslint": "^5.0.0",
    "tslint-config-prettier": "^1.1.0",
    "typescript": "^2.9.2",
    "tyranid-openapi": "^0.5.0",
    "tyranid-tdgen": "^0.5.17"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
